{
  "mcpServers": {
    "context7": {
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}"
      }
    },
    "playwright": {
      "command": "@bunxPath@",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "serena": {
      "command": "@uvxPath@",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "agent",
        "--enable-web-dashboard",
        "false"
      ]
    }
  }
}
