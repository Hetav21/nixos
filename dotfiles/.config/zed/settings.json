{
  "agent": {
    "default_model": {
      "model": "claude-3-5-sonnet-latest",
      "provider": "zed.dev"
    },
    "enabled": true,
    "inline_alternatives": [
      {
        "model": "gpt-3.5-turbo",
        "provider": "copilot_chat"
      }
    ]
  },
  "auto_install_extensions": {
    "Basher": true,
    "CSV": true,
    "Docker Compose": true,
    "Dockerfile": true,
    "Git Firefly": true,
    "GraphQL": true,
    "HTML": true,
    "Hyprlang": true,
    "LOG": true,
    "Prisma": true,
    "Python LSP": true,
    "SQL": true,
    "TOML": true,
    "nix": true
  },
  "auto_update": false,
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_features": {
    "calt": true,
    "kern": true,
    "liga": true
  },
  "buffer_font_size": 16.0,
  "languages": {
    "C++": {
      "format_on_save": "on",
      "show_edit_predictions": false,
      "tab_size": 2
    },
    "Python": {
      "show_edit_predictions": false
    }
  },
  "load_direnv": "shell_hook",
  "lsp": {
    "clangd": {
      "binary": {
        "arguments": [
          "--function-arg-placeholders=0",
          "--background-index",
          "--clang-tidy",
          "--log=verbose"
        ],
        "path": "/nix/store/sm2pmndi4srnb31b5a13bgg4il5xy1yr-clang-tools-19.1.7/bin/clangd",
        "path_lookup": true
      }
    },
    "nil": {
      "initialization_options": {
        "formatting": {
          "command": [
            "alejandra"
          ]
        }
      }
    }
  },
  "node": {
    "npm_path": "/nix/store/ijsy113yzy0mpcr1sf0773nz9v0r7hff-nodejs-22.17.1/bin/npm",
    "path": "/nix/store/ijsy113yzy0mpcr1sf0773nz9v0r7hff-nodejs-22.17.1/bin/node"
  },
  "show_edit_predictions": true,
  "show_whitespaces": "selection",
  "tabs": {
    "file_icons": true
  },
  "terminal": {
    "alternate_scroll": "off",
    "blinking": "off",
    "button": false,
    "copy_on_select": false,
    "detect_venv": {
      "on": {
        "activate_script": "default",
        "directories": [
          ".env",
          "env",
          ".venv",
          "venv"
        ]
      }
    },
    "dock": "bottom",
    "env": {
      "TERM": "ghostty"
    },
    "line_height": "comfortable",
    "option_as_meta": false,
    "shell": "system",
    "working_directory": "current_project_directory"
  },
  "theme": "Base16 untitled",
  "ui_font_family": "JetBrainsMono Nerd Font Mono",
  "ui_font_size": 16.0,
  "vim_mode": true
}
